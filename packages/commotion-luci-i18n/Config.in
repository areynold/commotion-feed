if PACKAGE_commotion-luci-i18n

config COMMOTION_LUCI_I18N_SPANISH
	bool "Enable Spanish language set"
	default n
	select PACKAGE_luci-i18n-spanish
	select PACKAGE_commotion-luci-i18n
